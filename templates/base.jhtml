<!DOCTYPE html>
<html>
<head>
{% set title = "SkyBlock" + version + " Challenge Manager " + site %}
{% set messages = get_flashed_messages() %}
<title>{{ title }}</title>
<script type="text/javascript" src="{{ url_for('static', filename='jquery-1.7.1.min.js') }}"></script>
{% block js %}{% endblock %}
<link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='style.css') }}" />
<link rel="shortcut icon" href="{{ url_for('static', filename='favicon.ico') }}">
</head>
{% if messages %}
<body onload="javascript:$('#responsediv').html('{% for m in messages %}{{ m }}<br />{% endfor %}').fadeIn().delay(1500).fadeOut()">
{% else %}
<body>
{% endif %}
<div id="bgdiv">
<div id="content">
<div id="responsediv"></div>
<h1>{{ title }}</h1>
<div id="nav">{% block nav %}{% endblock %}</div>
{% block content %}{% endblock %}
</div></div>
</body></html>
