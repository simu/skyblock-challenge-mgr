<!DOCTYPE html>
<html>
<head>
{% set title = "SkyBlock Manager " + version %}
<title>{{ title }}</title>
<script type="text/javascript" src="{{ url_for('static', filename='jquery-1.7.1.min.js') }}"></script>
<script type="text/javascript" src="{{ url_for('static', filename='save.js') }}"></script>
<link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='style.css') }}" />
</head>
<body>
<div id="bgdiv">
<div id="content">
<div id="responsediv"></div>
<h1>{{ title }}</h1>
<form name="dummy" onchange="javascript:store()">
<h2>Challenges:</h2>
<ul>
{% for c in challenges %}
<li>
    <input type="checkbox" name="{{ c.id }}" id="cb{{ c.id }}" {%if c.checked%}checked="checked"{%endif%} />
    {{ c.id+1 }}) {{ c.desc }}
</li>
{% endfor %}
</ul>
</form>
</div></div>
</body></html>
